//on modal show attach a click method to detect where you are clicking 
var modals = $('.generalmodal');
var isModalOpen = false;
var modalOpenedFirst;
var disableClick = false;
var newCheckHash = true;

$(document).ready(function () {
    //check if modal needs to be open on load
    checkHash();
    $(window).on('hashchange', function (e) {
        if (window.location.hash == "" || window.location.hash == undefined || window.location.hash == "#") {
            if (isModalOpen) {
                closeModals(modalOpenedFirst);
                return;
            }
        }
        if (newCheckHash) {
            checkHash();

        }
        //this is the case you are opening a new modal from inside a modal 
        else {
            closeModals(modalOpenedFirst);
            checkHash();
        }
    });

    $('.modal-exit-button.icon-x').on().off().click(function () {
        closeModals($(this).parent().parent().parent());
    });

    $('.background').click(function (e) {
        if (disableClick) {
            return;
        }
        closeModals($(this));
        window.location.hash = "#";
    });

    $('.modal-container').mouseenter(function () {
        disableClick = true;
    }).mouseleave(function () {
        disableClick = false;
    });
});



function checkHash() {
    //case for initialization 
    if (window.location.hash == "" || window.location.hash == undefined || window.location.hash == "#") {
        return;
    }
    for (i = 0; i < modals.length; i++) {
        var modalBackground = $($(modals[i]).children()[0]).children('.background')[0];

        var hash = '#' + $(modalBackground).attr('anchorId');
        if (hash == window.location.hash) {
            openModal(modalBackground);
        }
    }
    newCheckHash = false;
}

function openModal(targetModal) {

    $('html').addClass("disable-scroll");
    if (!isModalOpen) {
        $(targetModal).css("display", "flex");
        isModalOpen = true;
        modalOpenedFirst = targetModal;
        return;
    } else {
        if (targetModal.children[0] != undefined && !(targetModal.children.length > 1)) {
            modalOpenedFirst.appendChild(targetModal.children[0]);
            return;
        }
        return;

    }
}

function closeModals(parentBackground) {
    checkForVideos(parentBackground);
    isModalOpen = false;
    modalOpenedFirst = null;
    $(parentBackground).css("display", "none");
    newCheckHash = true;
    scrollTop =
        window.pageYOffset || document.documentElement.scrollTop;
    scrollLeft =
        window.pageXOffset || document.documentElement.scrollLeft,

        // if any scroll is attempted,
        // set this to the previous value
        window.onscroll = function () {
            window.scrollTo(scrollLeft, scrollTop);
        };
    //remove scrolling prevention once modal is finished closing
    setTimeout(function () {
        window.onscroll = function () { };
    }, 50);
    $('html').removeClass("disable-scroll");
}

function checkForVideos(parentBackground) {
    try {
        if (parentBackground.find('iframe') == null || parentBackground.find('iframe') == undefined) {
            return;
        }
        parentBackground.find('iframe').each(function () {
            $(this).attr('src', $(this).attr('src'));
        });
    } catch (e) {
        return;
    }
}
