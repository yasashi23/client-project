(function() {
  var active = document.activeElement;

  function hideVideoTitle() {
    var focusedElement = $(active),
      videoItem = focusedElement.closest('.video__item');

    if (videoItem.length) {
      videoItem.find('p').addClass('hide-on-play');
    }
  }

  $('.video-embed video').on('click touch', function() {
    active = document.activeElement;
    hideVideoTitle();
  });

  setInterval(function() {
    if (active !== document.activeElement) {
      active = document.activeElement;
      // if this is a video, find that parent element and remove the overlay
      hideVideoTitle();
    }
  }, 300);
})();

