var map;
var longitude = $('.google-map').data("central-lng");
var latitude = $('.google-map').data("central-lat");
var zoom = $('.google-map').data("zoom");
var type = $('.google-map').data("map-type");
var hybridTilt = $('.google-map').data("hybrid-tilt");
var satelliteTilt = $('.google-map').data("satellite-tilt");
var setTilt = 0;
if (type == "satellite" && satelliteTilt)  {
    setTilt = 45;
}
if (type == "hybrid" && hybridTilt) {
    setTilt = 45;
}

function initMap() {
    map = new google.maps.Map(document.getElementById("map"), {
    center: { lat: parseFloat(latitude), lng: parseFloat(longitude)},
    zoom: parseInt(zoom),
    mapTypeId: type,
    tilt: setTilt,
    });

    window.googleMap = map;
    createMarkers();
}

function createMarkers() {
        
    var data = $('.google-map').data("json");
     if ($('.google-map').data("formatting") == "json") {
        data = JSON.stringify(data);
        data = JSON.parse(data);
    } else {
        try {
            data = JSON.parse(data.replaceAll('\n','\\n'));
        } catch (e) {
            console.log("google map points did not come in the form of a string");
        }
    }
    data.forEach((point, i) => {
        var pointTitle = point.title != 'null' ? point.title : '';
        var pointIcon = point.customIcon != 'null' ? point.customIcon : '';
        var pointContent = point.content != 'null' ? point.content : '';
        const infowindow = new google.maps.InfoWindow({
            content: "<h5 class='marker--title mb-3'>"+ pointTitle +"</h5>" + "<p class='marker-content'>" + pointContent + "</p>",
        });

        const marker = new google.maps.Marker({
            position: {lat : parseFloat(point.latitude), lng: parseFloat(point.longitude)},
            map,
            title: pointTitle,
            icon: pointIcon,
            optimized: false,
            });
            // Add a click listener for each marker, and set up the info window.
            marker.addListener("click", () => {
            infowindow.open({
                anchor: marker,
                map,
                shouldFocus: false,
            });
        });

    });
}

window.addEventListener('load', function () {
    window.initMap = initMap;
})
