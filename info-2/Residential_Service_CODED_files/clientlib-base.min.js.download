var paginations = [];

function initPagination($pagination, $items, itemsPerPage, $visibleItems) {
    $pagination.html('');

    var paginationPrev = $('<div class="pagination-primary__prev pagination-primary__arrow icon-leftarrow disabled"></div>');
    $pagination.append(paginationPrev);

    var pageCount = Math.ceil($items.length / itemsPerPage);
    for (var i = 0; i < pageCount; i++) {
        var paginationItem = $('<div class="pagination-primary__item" data-number="' + (i + 1) + '"></div>');
        paginationItem.text(i + 1);
        if (i == 0) {
            paginationItem.addClass('active');
        }
        $pagination.append(paginationItem);
    }

    var paginationNext = $('<div class="pagination-primary__next pagination-primary__arrow icon-rightarrow"></div>');
    $pagination.append(paginationNext);

    // update the visible items text, this will always begin at item 1 since we're initializing
    if ($visibleItems) {
        $visibleItems.addClass('hidden');

        var firstItem = 1,
            totalItems = $items.length,
            lastItem = $items.length > itemsPerPage ? itemsPerPage : $items.length;
        $visibleItems.find('span').html(firstItem + ' &ndash; ' + lastItem + ' of ' + totalItems);

        if ($items.length > 0) {
            $visibleItems.removeClass('hidden');
        }
    }

    $items.each(function (i) {
        if (i >= itemsPerPage) {
            $(this).hide();
        }
    });

    if ($items.length > itemsPerPage) {
        $pagination.show();
    } else {
        $pagination.hide();
    }

    // reset page number
    var pagination = {};
    pagination.pageNumber = 1;
    pagination.$pagination = $pagination;
    pagination.$items = $items;
    pagination.$visibleItems = $visibleItems;
    pagination.itemsPerPage = itemsPerPage;
    pagination.pageCount = pageCount;
    paginations.push(pagination);
}

function changePage(pagination, pageNumber) {
    // update page number, calculate min and max index
    pagination.pageNumber = pageNumber;
    var minIndex = ((pageNumber * pagination.itemsPerPage) - pagination.itemsPerPage);
    var maxIndex = minIndex + pagination.itemsPerPage;
    // hide items accordingly
    pagination.$items.each(function (i) {
        if (i < minIndex || i >= maxIndex) {
            $(this).hide();
        } else {
            $(this).show();
        }
    });

    // update visible items based on min and max indices
    if (pagination.$visibleItems) {
        pagination.$visibleItems.addClass('hidden');

        var firstItem = minIndex + 1,
            totalItems = pagination.$items.length,
            lastItem = maxIndex > totalItems ? totalItems : maxIndex;
        pagination.$visibleItems.find('span').html(firstItem + ' &ndash; ' + lastItem + ' of ' + totalItems);

        if (pagination.$items.length > 0) {
            pagination.$visibleItems.removeClass('hidden');
        }
    }

    // set active page
    pagination.$pagination.find('.pagination-primary__item.active').removeClass('active');
    pagination.$pagination.find('[data-number=' + pagination.pageNumber + ']').addClass('active');

    // determine whether or not to disable the prev arrow
    if (pagination.pageNumber == 1) {
        pagination.$pagination.find('.pagination-primary__prev').addClass('disabled');
    } else {
        pagination.$pagination.find('.pagination-primary__prev').removeClass('disabled');
    }

    // determine whether or not to disable the next arrow
    if (pagination.pageNumber == pagination.pageCount) {
        pagination.$pagination.find('.pagination-primary__next').addClass('disabled');
    } else {
        pagination.$pagination.find('.pagination-primary__next').removeClass('disabled');
    }

    // scroll back up to top of component
    var navHeight;
    if ($(window).outerWidth() < 992) {
        navHeight = 50;
    } else {
        navHeight = 112;
    }
    var scrollTo = pagination.$items.first().closest('.component').offset().top - 24 - navHeight;
    if (scrollTo < 0) {
        scrollTo = 0;
    }
    $(window).scrollTop(scrollTo);
}

function getPagination($child) {
    var associatedPagination;
    for (var i = 0; i < paginations.length; i++) {
        if (paginations[i].$pagination.is($child.closest('.pagination-primary'))) {
            associatedPagination = paginations[i];
        }
    }

    return associatedPagination;
}

$(function () {
    $('body').on('click touch', '.pagination-primary__item', function (e) {
        e.stopImmediatePropagation();
        if (!$(this).hasClass('active')) {
            var associatedPagination = getPagination($(this));
            var num = parseInt($(this).attr('data-number'));
            changePage(associatedPagination, num);
        }
    });

    $('body').on('click touch', '.pagination-primary__arrow', function (e) {
        e.stopImmediatePropagation();
        if (!$(this).hasClass('disabled')) {
            var associatedPagination = getPagination($(this));
            var num;
            if ($(this).hasClass('pagination-primary__prev')) {
                num = associatedPagination.pageNumber - 1;
            } else {
                num = associatedPagination.pageNumber + 1;
            }

            changePage(associatedPagination, num);
        }
    });
});
